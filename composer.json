    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "^5.2",
        "illuminate/console": "^5.2",
        "illuminate/filesystem": "^5.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "RyanChan",
            "email": "rc@riseno.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Riseno\\Localizable\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "illuminate/database": "^5.2",
        "phpunit/phpunit": "^5.1"
    }
